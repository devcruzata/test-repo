<div class="modal fade main-modaldiv" id="exampleModal" tabindex="-1" role="dialog"
    aria-labelledby="exampleModalLabel">
    <div class="modal-dialog" role="document">
        <div class="modal-content">
            <div class="modal-header custom-header">
                <button type="button" class="close" data-dismiss="modal" aria-label="Close" id="cross_btn">
                    <span aria-hidden="true">
                        <img src="@Url.Content("~/Content/images/cross-icon.png")" /></span></button>
                <h4 class="modal-title" id="exampleModalLabel">
                    New message</h4>
            </div>
            
                <div class="modal-body">

                    <form action="/Home/AddQueryDetails" method="post" id="frm_data">
                    <div class="form-group col-sm-12">
                        <input type="text" class="form-control input-custom" placeholder="Your Name" id="PersonName" name="PersonName" >
                        <div id="error_msg_name"  style="display:none;">
                            <p style="color: red;">Please Enter Your Name</p>
                        </div>
                    </div>

                    <div class="form-group col-sm-6">
                        <input type="text" class="form-control input-custom" placeholder="Email" id="Email" name="Email" >
                        <div id="error_msg_email" style="display:none;">
                            <p style="color: red;">Please Enter Valid Email</p>
                        </div>
                    </div>
                    <div class="form-group col-sm-6">
                        <input type="text" class="form-control input-custom" placeholder="Phone Number" id="ContactNumber" name="ContactNumber" >
                        <div id="error_msg_no" style="display:none;">
                            <p style ="color: red;"> Contact Number Should be 10 characters</p>
                        </div>
                    </div>
                    <div class="form-group col-sm-12">
                        <input type="text" class="form-control input-custom" placeholder="Address" id="Address" name="Address" >
                        <div id="error_msg_address" style="display:none;">
                            <p style="color: red;">Please Enter Valid Address</p>
                        </div>
                    </div>
                    <div class="form-group col-sm-6">
                        <input type="text" class="form-control input-custom" placeholder="City" id="City" name="City" >
                        <div id="error_msg_city" style="display:none;">
                            <p style="color: red;">Please Enter Valid City Name</p>
                        </div>
                    </div>
                    <div class="form-group col-sm-6">                        
                        <select id="State" class="form-control input-custom" name="State">
                            <option value="">Select State</option>
                            <option value="Andaman and Nicobar Islands">Andaman and Nicobar Islands</option>
                            <option value="Andhra Pradesh">Andhra Pradesh</option>
                            <option value="Arunachal Pradesh">Arunachal Pradesh</option>
                            <option value="Assam">Assam</option>
                            <option value="Bihar">Bihar</option>
