@model IEnumerable<Project.ViewModel.ServicesListingViewModel>
@{
    Layout = null;
    ViewBag.Title = "Online Kaarigar | Service";
}

<!doctype html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link rel="icon" type="image/png" href="@Url.Content("~/Content/images/favicon.png")" />
    <link href="@Url.Content("~/Content/css/bootstrap.min.css")" rel="stylesheet" />
    <link rel="stylesheet" href="@Url.Content("~/Content/css/style.css")" type="text/css" />
    <link href="@Url.Content("~/Content/css/font-awesome.min.css")" rel="stylesheet" />
    <link href="@Url.Content("~/Content/PopUp/PopUpStyle.css")" rel="stylesheet" type="text/css" />
    <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
    <!-- WARNING: Respond.js doesn't work if you view the page via file:// -->
    <!--[if lt IE 9]>
      <script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
      <script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
    <![endif]-->
    <!-- jQuery (necessary for Bootstrap's JavaScript plugins) -->
    <script src="@Url.Content("~/Scripts/js/jquery.min.js")"></script>
    <!-- Include all compiled plugins (below), or include individual files as needed -->
    <script src="@Url.Content("~/Scripts/js/bootstrap.min.js")"></script>
    <script>
        (function (i, s, o, g, r, a, m) {
            i['GoogleAnalyticsObject'] = r; i[r] = i[r] || function () {
                (i[r].q = i[r].q || []).push(arguments)
            }, i[r].l = 1 * new Date(); a = s.createElement(o),
            m = s.getElementsByTagName(o)[0]; a.async = 1; a.src = g; m.parentNode.insertBefore(a, m)
        })(window, document, 'script', '//www.google-analytics.com/analytics.js', 'ga');

        ga('create', 'UA-72566421-1', 'auto');
        ga('send', 'pageview');

    </script>
</head>
<body>
    <header class="header">
        <div class="container">
            <div class="row nav-row">
                <div class="navbar-header">
                    <div class="mobile-logo"><a href="#"><img src="~/Content/images/logo.png" /></a></div>
                    <button type="button" class="navbar-toggle menu-button" data-toggle="collapse" data-target="#myNavbar"> <span class="fa fa-bars"></span> </button>
                </div>

                <div class="col-md-12 menus-div">
                    <nav class="collapse navbar-collapse nav-custom" id="myNavbar" role="navigation">
                        <ul class="nav navbar-nav navbar-left menu">
                            <li><a href="/Home/Index" class="page-scroll active">Home</a></li>
                            <li><a href="/Home/#services" class="page-scroll">services</a></li>
                            <li><a href="/Home/#aboutus" class="page-scroll">about </a></li>
                            <li><a href="/Home/#contactus" class="page-scroll">contact us</a></li>
                        </ul>
                        <ul class="logo-div-center">
                            <li class="logo-div">
                                <a class="navbar-brand logo" href="#"><img src="~/Content/images/logo.png" /></a>
                            </li>
                        </ul>
                        @*<ul class="nav navbar-nav navbar-right menu">
                                <li class="search-li"><input class="form-control header-input" placeholder="search" /></li>
                                <li><a data-target="#exampleModal" data-toggle="modal" class="btn btn-default btn-query launch-modal" href="#">Request a Quote</a></li>
                            </ul>*@
                        @Html.Action("Header", "Home")
                    </nav>
                </div>
            </div>
        </div>
    </header>
    @if (Model.ServicesList != null && Model.ServicesList.Any())
    {
        <div class="container-fluid work" id="services">
            <div class="container">
                <div class="row" id="service_row">

                    <div class="col-md-12 col-sm-12 col-xs-12 service-list">
                        <h2 class="text-center heading-text">
                            Services
                        </h2>
                        <div class="row">
                            <div class="col-md-4">
                                <input type="search" name="search" class="form-control search-input search input-search" id="search" placeholder="search..." style="width:363px;" />
                            </div>
                            <div class="col-md-2">
                                <button class="btn  btn_search" id="search_btn">Search</button>
                            </div>
                        </div>
                        @*<div class="col-sm-12 text-left ">
                                <input type="search" name="search" class="form-control search-input search input-search" id="search" placeholder="search..." style="width:363px;" />
                                <button class="btn btn-default">Search</button>
                            </div>*@
                        <div id="service">
                            @for (int i = 0; i < Model.ServicesList.Count; i++)
                            {
                                @Html.Partial("_ServiceListHomePartial", Model.ServicesList[i])
                            }
                        </div>
                    </div>
                </div>
            </div>
        </div>
    }



    <div class="container-fluid countspace" id="contactus">
        <div class="container">
            <div class="row">
                <div class="col-md-12 contact-div">
                    <!--<h2 class="text-center heading-text">Contact us</h2>-->
                    <div class="col-sm-6 contact-div-left">
                        <h4>About</h4>
                        <ul class="footer-menu">

                            <li><a href="/Home/Services">Services</a></li>
                            <li><a href="/Home/FAQ">FAQs</a></li>
                            <li><a href="/Home/Policies">Privacy Policy</a></li>
                            <li><a href="/Home/Terms">Terms of Use</a></li>
                        </ul>
                    </div>
                    <div class="col-sm-6 contact-div-right">
                        <h4>Contact Us</h4>
                        <p><i class="fa fa-map-marker"></i>&nbsp; 110 Onam Plaza, Industry House, AB Road (Indore) </p>
                        <p><i class="fa fa-phone"></i>&nbsp; +91-756-666-6022 </p>
                        <p><i class="fa fa-envelope"></i>&nbsp; Info@onlinekarigar.com </p>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <div class="container-fluid footer">
        <div class="col-sm-12">
            <div class="col-md-12">
                <p>
                    Onlinekarigar.com © 2016 All rights reserved.
                </p>
            </div>
        </div>
    </div>
	
</body>

<script type="text/javascript">
    $(document).ready(function () {
        $('#search_btn').click(function (event) {
            alert();
            var searchText = $('#search').val();
            //var items=$("#Service_Name")
            //$.each(items,function (index,item) {

            //    alert($(item).attr("name"));
            //});
            if ($('#search').val() != "") {
                alert();
                $.post("/Home/ServiceListHomePage", { queryText: searchText });
            }


        });
      
    });



  
</script>